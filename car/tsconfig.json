{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2015", "DOM"]
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}
